{
  "title": "Apache Ignite 3 Java API Primer",
  "description": "Practical Patterns for Distributed Data Programming",
  "author": "Apache Ignite Community",
  "language": "en",
  "gitbook": ">=3.0.0",
  "plugins": [
    "mermaid-md-adoc",
    "anchors",
    "page-toc-button",
    "expandable-chapters",
    "chapter-fold",
    "code",
    "blockquote-callout"
  ],
  "pluginsConfig": {
    "page-toc-button": {
      "maxTocDepth": 3,
      "minTocSize": 2
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Times",
    "paperSize": "letter",
    "chapterMark": "pagebreak",
    "pageBreaksBefore": "/",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  },
  "variables": {
    "version": "1.0.0"
  }
}
